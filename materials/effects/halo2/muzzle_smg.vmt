"UnlitGeneric"
{
	"$basetexture"	"effects/halo2/muzzle_smg"
	"$additive"	1
	"$vertexcolor"	1
	"$vertexalpha"	1
	"$translucent" 				"1"

"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "30"
      }
   }

}