	$phongexponent	44
	$phongboost	13
                 
                 "$phongfresnelranges" "[ 0.05 0.3 1 ]" 
	
	$lightwarptexture	"snowysnowtime\2k\h3\lw_metaldull"

	$envmap		"env_cubemap"
	$normalmapalphaenvmapmask 1
	$cubemaptint	"[ 0 0 0 ]"
