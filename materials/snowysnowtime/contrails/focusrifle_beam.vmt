"UnlitGeneric"
{
	"$basetexture" "snowysnowtime/contrails/focusrifle_beam"
	"$additive" "1"
	"$vertexalpha" "1"
	"$vertexcolor" 1
	"$translucent" "1"
	"$color2" "[0.3 1 1]"
	"Proxies"
	{
	"AnimatedTexture"
	{
	"animatedTextureVar" "$basetexture"
	"animatedTextureFrameNumVar" "$frame"
	"animatedTextureFrameRate" "20"
	}
	"TextureScroll"
    {
    "texturescrollvar" "$basetexturetransform"
    "texturescrollrate" 20
    "texturescrollangle" -90
    }
}