"UnlitGeneric"
{
	"$basetexture" "snowysnowtime/2k/h3/br/hud/numbers_plate2"
	"$additive" "1"
	$decal "1"
	Proxies // proxies are listed inside this block
	{
		ArcCW_HALOCE_AR
		{
			resultVar 1
			prefixstring "snowysnowtime/ma5e/hud/numbers_plate"
		}
	drc_PlayerColor
		{
			resultVar		$color2 // pass the player color value to Gmod
			default		1 1 1 // unused for now.
		}
	Clamp // clamp the chosen color between 1% and 125% (usually)
		{
			min 0.1
			max 1.15
			srcVar1 "$color2"
			resultVar "$color2"
		}
	}
}
