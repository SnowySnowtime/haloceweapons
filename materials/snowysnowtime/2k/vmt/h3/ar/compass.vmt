UnlitGeneric
{
	"$basetexture"	"snowysnowtime/2k/h3/compass"

	$additive 1
	$translucent 1
	$model 1

	$angle		 0
	$translate		 "[0 0]"
	$center		 "[.5 .5]"

	$nocull 1

	Proxies
	{
		vuth_Compass
		{
			resultVar $angle
		}
		TextureTransform
		{
			"translateVar" "$translate"
                		"rotateVar" "$angle"
                		"centerVar" "$center"
                		"resultVar" "$basetexturetransform"
		}
	}
}
