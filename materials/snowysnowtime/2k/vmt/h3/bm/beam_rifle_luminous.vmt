"VertexLitGeneric"
{
	"$basetexture" "snowysnowtime/2k/h3/bm/airlock_field_mask"
	"$texturetransform" "center 0 0 scale 0.5 0.5 rotate 0 translate 0 0"

	"$model" "1"
	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "0.5"
	"$phongalbedotint" "1"
	"$phongtint" "[1 0.85 1]"
	"$phongfresnelranges"	"[0.5 6 15]"

	"$selfillum" "1"
	"$selfillumtint" "[3 1 3]"
	"$selfillummask" "snowysnowtime/2k/h3/bm/airlock_field_mask"

	"$color2" "[3 1 3]"
	"$ambientocclusion" "1"
	"$detail" "snowysnowtime/2k/h3/bm/airlock_field_mask"
	"$detailscale" "10"
	"$detailblendfactor" "0.2"
	"$detailblendmode" "5"

"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$bumpmap"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 20
		}

		"TextureScroll"
		{


          "LinearRamp"
            {
                "rate" 4
                "initialValue" 0.0
                "resultVar" "$angle"
            }


			"texturescrollvar" "$baseTextureTransform"
			"texturescrollrate" .1
			"texturescrollangle" 90
"texturescrollvar" "$baseTextureTransform"
			"texturescrollrate" -.00
			"texturescrollangle" 90


}
			
			
	}
}		
	


