"UnlitGeneric"
{
	$no_draw 0
	$basetexture	"snowysnowtime/energysword/fx"
	$additive 1

	$translucent	1
	$nocull		1

	$color2	"[ 3 0.5 0.5]"

	"Proxies"
	{
	"AnimatedTexture"
	{
	"animatedTextureVar" "$basetexture"
	"animatedTextureFrameNumVar" "$frame"
	"animatedTextureFrameRate" "30"
	}
	"TextureScroll"
	{
		"textureScrollVar" "$basetexturetransform"
		"textureScrollRate" -0.175
		"textureScrollAngle" 0
		}
	}
}


