"UnlitGeneric"
{
	"$basetexture" "snowysnowtime/sword_det"
	"$detail" "snowysnowtime/sword_det"
	"$color2" "[ 1 1.5 2 ]"
	"$translucent" "1"
	"$additive" "1"
	"$nodraw" "1"
	"Proxies"
	{
	"AnimatedTexture"
	{
	"animatedTextureVar" "$basetexture"
	"animatedTextureFrameNumVar" "$frame"
	"animatedTextureFrameRate" "20"
	}
	"TextureScroll"
    {
    "texturescrollvar" "$basetexturetransform"
    "texturescrollrate" -0.20
    "texturescrollangle" 45
    }	
}
