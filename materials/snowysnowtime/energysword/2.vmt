"UnlitGeneric"
{
	"$basetexture" "snowysnowtime/sword_plasma"
	"$detail" "snowysnowtime/sword_det"
	"$color2" "[ 0.1 0.1 0.1 ]"
	"$translucent" "1"
	"$additive" "1"
	"$nodraw" "1"
	"Proxies"
	{
	"AnimatedTexture"
	{
	"animatedTextureVar" "$basetexture"
	"animatedTextureFrameNumVar" "$frame"
	"animatedTextureFrameRate" "20"
	}
	"TextureScroll"
    {
    "texturescrollvar" "$basetexturetransform"
    "texturescrollrate" -0.10
    "texturescrollangle" 0
    }	
}
